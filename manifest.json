{
  "manifest_version": 2,

  "name": "空污污流流 - Taiwan Air PM2.5 Map on Windyty",
  "description": "A chrome extension draws air pollution data of Taiwan on Windyty",
  "version": "1.0",

  "background": {
    "scripts": [
      "background.js"
    ]
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Click to Draw"
  },

  "permissions": [
    "activeTab",
    "https://windy.com/*",
    "https://airmap.g0v.asper.tw/*"
  ],
  "icons": {
    "96": "icon.png"
  }
}
